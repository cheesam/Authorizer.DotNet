{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authorizer": {
    "AuthorizerUrl": "https://your-authorizer-instance.com",
    "RedirectUrl": "https://localhost:5001/auth/callback",
    "ClientId": "your-client-id",
    "HttpTimeout": "00:00:30",
    "UseSecureCookies": true
  }
}